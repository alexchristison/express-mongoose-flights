<%- include('../partials/header') %>

<table>
  <thead>
      <tr>
          <th>Flight</th>
          <th>Airport</th>
          <th>Details</th>
      </tr>
  </thead>
  <tbody>
  <% flights.forEach(function(flight) { %>
    <tr>
      <td><%= flight.airline %></td>
      <td><%= flight.airport %></td>
      <td><a href="/flights/<%= flight.id %>">DETAILS</a></td>
    </tr>
  <% }) %>
</tbody> 
</table>

<%- include('../partials/header') %>